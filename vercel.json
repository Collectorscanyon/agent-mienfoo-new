
{
  "version": 2,
  "builds": [
    { "src": "api/index.ts", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST",
        "Access-Control-Allow-Headers": "X-Neynar-Signature, Content-Type"
      },
      "dest": "/api/routes/webhook.ts"
    }
  ]
}
